{
  "name": "@llm-utils/chatgpt-api",
  "version": "1.1.0",
  "description": "ChatGPT API",
  "scripts": {
    "build-dts": "rollup -c rollup.dts.config.cjs",
    "build-js": "microbundle --define process.env.NODE_ENV=production,PKG_VERSION=$npm_package_version",
    "build": "pnpm build-js && pnpm build-dts",
    "watch": "microbundle watch --define process.env.NODE_ENV=development,PKG_VERSION=$npm_package_version --compress false",
    "dev": "pnpm watch",
    "prepack": "pnpm run build",
    "start": "pnpm build",
    "clean": "del build",
    "pre-commit": "lint-staged",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhzLuke96/node-chatgpt-api.git"
  },
  "author": "",
  "license": "GPLv3",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.module.mjs",
  "unpkg": "dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/zhzLuke96/node-chatgpt-api/issues"
  },
  "homepage": "https://github.com/zhzLuke96/node-chatgpt-api#readme",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.0.3",
    "eventsource-parser": "^0.1.0",
    "gpt3-tokenizer": "^1.1.5",
    "https-proxy-agent": "^5.0.1",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "node-fetch-retry": "^2.0.1",
    "p-timeout": "^6.1.1",
    "prettier": "^2.8.4",
    "rollup": "^3.25.3",
    "rollup-plugin-dts": "^5.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "openai",
    "chatgpt",
    "chat",
    "chatbot",
    "chatgpt3",
    "gpt3",
    "gpt",
    "ai",
    "bot"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
